<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="361.0" prefWidth="486.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.un.sdgs.terratales.FavoritesController">
   <children>
      <Label fx:id="userLabel" layoutX="28.0" layoutY="17.0" text="User's Favorites" />
      <ScrollPane fx:id="scrollPane" layoutX="23.0" layoutY="44.0" prefHeight="250.0" prefWidth="146.0">
         <content>
            <VBox fx:id="favoritesVbox" prefHeight="318.0" prefWidth="144.0">
               <children>
                  <Button alignment="TOP_LEFT" mnemonicParsing="false" prefHeight="35.0" prefWidth="143.0" text="PlaceName">
                     <graphic>
                        <ImageView fitHeight="25.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true" />
                     </graphic>
                  </Button>
               </children>
            </VBox>
         </content>
      </ScrollPane>
      <Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="197.0" layoutY="140.0" prefHeight="18.0" prefWidth="259.0" text="Name" textAlignment="CENTER" />
      <Label fx:id="placeLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="197.0" layoutY="169.0" prefHeight="18.0" prefWidth="266.0" text="Place" textAlignment="CENTER" />
      <Button layoutX="223.0" layoutY="314.0" mnemonicParsing="false" onAction="#onBackPress" text="Back" />
      <Label fx:id="descriptionLabel" layoutX="197.0" layoutY="196.0" prefHeight="71.0" prefWidth="259.0" text="Description" wrapText="true" />
      <ImageView fx:id="locationImage" fitHeight="101.0" fitWidth="127.0" layoutX="264.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
